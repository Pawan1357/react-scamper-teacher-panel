{
  "name": "react-scamper-school",
  "private": true,
  "author": "(saurav.sapariya@agileinfoways.com)",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": " vite",
    "build": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "test": "vitest run && vitest --ui",
    "coverage": "vitest run --coverage ",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 10",
    "preview": "vite preview",
    "prettier": "prettier --check \"**/*.{ts,tsx,css}\" --config ./.prettierrc",
    "prettier:fix": "prettier --write \"**/*.{ts,tsx,css}\" --config ./.prettierrc",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@react-oauth/google": "^0.12.2",
    "@tanstack/react-query": "^5.90.2",
    "@testing-library/react": "16.0.0",
    "antd": "5.18.0",
    "axios": "1.7.2",
    "prettier": "3.3.3",
    "react": "^18.2.0",
    "react-custom-roulette": "^1.4.1",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-number-format": "^5.4.4",
    "react-router-dom": "6.23.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "6.1.11",
    "zustand": "4.5.2"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.90.2",
    "@testing-library/jest-dom": "^6.4.5",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-pwa": "^0.20.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run lint",
      "npm run prettier"
    ]
  }
}
